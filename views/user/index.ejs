<h1>Users List</h1>
<table>
    <thead>
    <tr>
        <th>Name</th>
        <th>Email</th>
    </tr>
    </thead>
    <tbody>
    <% users.forEach(user => { %>
        <tr>
            <td><%= user.username %></td>
            <td><%= user.email %></td>
        </tr>
    <% }); %>
    </tbody>
</table>
